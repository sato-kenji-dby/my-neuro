{
  "name": "live2d",
  "private": true,
  "version": "0.1.0alpha",
  "main": "electron.cjs",
  "type": "module",
  "build": {
    "appId": "com.you.live2d",
    "productName": "Live2D",
    "directories": {
      "output": "release"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.github",
      "!LICENSE",
      "!README.md",
      "!src/",
      "!docs/",
      "!.vscode/",
      "!vite.*.ts",
      "!vitest.*.ts",
      "!svelte.config.js",
      "!tailwind.config.js",
      "!tsconfig.json",
      "!.npmrc",
      "!.eslintrc.cjs",
      "!eslint.config.js",
      "!.prettierrc",
      "!test-data",
      "!*.db",
      "build/"
    ]
  },
  "scripts": {
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "npx rimraf dist dist-electron",
    "postinstall": "electron-builder install-app-deps",
    "build:frontend": "vite build",
    "build:electron:compile": "vite build --config vite.main.config.ts",
    "build:electron:rename": "echo 'Renaming handled by Vite config'",
    "build:electron": "npm run build:electron:compile",
    "build": "npm run clean && npm run build:frontend && npm run build:electron",
    "dev:frontend": "npx vite dev",
    "watch:electron": "vite build --config vite.main.config.ts --watch",
    "start:electron:dev": "cross-env ELECTRON_START_URL=http://localhost:5173 npx nodemon",
    "dev:prepare": "npm run clean",
    "dev:run": "concurrently --raw \"set PYTHONIOENCODING=utf-8 && chcp 65001 && npm run dev:frontend\" \"set PYTHONIOENCODING=utf-8 && chcp 65001 && npm run watch:electron\" \"set PYTHONIOENCODING=utf-8 && chcp 65001 && npm run start:electron:dev\"",
    "dev": "npm run dev:prepare && npm run dev:run",
    "dev:electron:start": "nodemon --watch \"dist-electron/**/*.cjs\" --ext cjs --exec \"electron electron.cjs\"",
    "start": "electron electron.cjs",
    "build:app": "npm run build && npx electron-builder",
    "generate-licenses": "npm-license-crawler --onlyDirectDependencies --json licenses.json",
    "dev:app-protocol": "npm run build:frontend && electron ."
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@google/genai": "^1.10.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-typescript": "^12.1.4",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron": "^1.4.38",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.12",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.3.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.10",
    "npm-license-crawler": "^0.2.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "renamer": "^5.0.2",
    "rimraf": "^6.0.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.36.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "electron": "^28.0.0",
    "express": "^5.1.0",
    "https-proxy-agent": "^7.0.6",
    "lucide-svelte": "^0.525.0",
    "node-fetch": "^2.7.0",
    "pixi-live2d-display": "^0.4.0",
    "pixi.js": "^6.5.10",
    "socks-proxy-agent": "^8.0.5"
  }
}
