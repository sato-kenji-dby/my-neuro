}

/* 侧边栏样式 - 浅灰渐变 */
QFrame#sidebar {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(235, 233, 225, 200), stop:0.5 rgba(230, 228, 220, 200), stop:1 rgba(225, 223, 215, 200));
    border: 1px solid rgba(200, 195, 185, 150);
    border-radius: 20px;
    margin: 5px;
}

/* 导航按钮样式 */
QPushButton#navButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(220, 218, 210, 180), stop:1 rgba(215, 213, 205, 180));
    color: rgb(114, 95, 77);
    text-align: left;
    padding: 20px 25px;
    border: 1px solid rgba(180, 175, 165, 120);
    border-radius: 12px;
    font-size: 20px;
    font-family: "Microsoft YaHei", "SimHei", sans-serif;
    font-weight: bold;
    margin: 5px 0px;
}

QPushButton#navButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(210, 205, 195, 220), stop:1 rgba(200, 195, 185, 220));
    color: rgb(60, 55, 45);
    border: 1px solid rgba(160, 155, 145, 180);
    box-shadow: 0px 2px 8px rgba(140, 135, 125, 60);
}

QPushButton#navButton:checked {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(190, 185, 175, 255), stop:1 rgba(180, 175, 165, 255));
    color: rgb(40, 35, 25);
    font-weight: bold;
    border: 2px solid rgba(120, 115, 105, 220);
    box-shadow: 0px 2px 15px rgba(100, 95, 85, 100);
}

/* 主内容区域 - 浅色毛玻璃效果 */
QFrame#contentArea {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 rgba(240, 238, 230, 200), stop:0.5 rgba(235, 233, 225, 200), stop:1 rgba(230, 228, 220, 200));
    border: 1px solid rgba(200, 195, 185, 180);
    border-radius: 20px;
    margin: 10px;
    padding: 0px;
}

/* 堆叠控件背景 */
QStackedWidget {
    background-color: transparent;
}

/* 页面背景 */
QWidget#page, QWidget#page_2, QWidget#page_3, QWidget#page_4, QWidget#page_5 {
    background-color: transparent;
}

/* 标签样式 */
QLabel {
    color: rgb(114, 95, 77);
    font-size: 22px;
    font-family: "Microsoft YaHei", "SimHei", sans-serif;
    font-weight: bold;
    background-color: transparent;
}

/* 输入框样式 - 浅色毛玻璃效果 */
QLineEdit {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(250, 249, 245, 255), stop:1 rgba(248, 247, 242, 255));
    border: 1px solid rgba(180, 175, 165, 200);
    border-radius: 14px;
    padding: 12px 18px;
    color: rgb(114, 95, 77);
    font-size: 22px;
    font-family: "Microsoft YaHei", "SimHei", sans-serif;
    font-weight: bold;
}

QLineEdit:focus {
    border: 2px solid rgb(140, 135, 125);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(215, 213, 205, 255), stop:1 rgba(225, 223, 215, 255));
    box-shadow: 0px 0px 15px rgba(120, 115, 105, 80);
}

/* 文本编辑框样式 - 浅色毛玻璃效果 */
QTextEdit {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(250, 249, 245, 255), stop:1 rgba(248, 247, 242, 255));
    border: 1px solid rgba(180, 175, 165, 200);
    border-radius: 14px;
    padding: 15px;
    color: rgb(114, 95, 77);
    font-size: 22px;
    font-family: "Microsoft YaHei", "SimHei", sans-serif;
    font-weight: bold;
}

QTextEdit:focus {
    border: 2px solid rgb(140, 135, 125);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(245, 244, 240, 255), stop:1 rgba(248, 247, 242, 255));
    box-shadow: 0px 0px 15px rgba(120, 115, 105, 80);
}

/* 复选框样式 */
QCheckBox {
    color: rgb(114, 95, 77);
    font-size: 22px;
    font-family: "Microsoft YaHei", "SimHei", sans-serif;
    font-weight: bold;
    spacing: 12px;
    margin: 15px 0px;
    background-color: transparent;
}

QCheckBox::indicator {
    width: 20px;
    height: 20px;
    border-radius: 4px;
    border: 2px solid rgb(140, 135, 125);
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 rgba(230, 228, 220, 255), stop:1 rgba(240, 238, 230, 255));
}

QCheckBox::indicator:checked {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 rgba(120, 115, 105, 255), stop:1 rgba(100, 95, 85, 255));
    border: 2px solid rgb(80, 75, 65);
}

QCheckBox::indicator:hover {
    border: 2px solid rgb(100, 95, 85);
    box-shadow: 0px 0px 8px rgba(120, 115, 105, 60);
}

/* 按钮样式 - 浅色渐变 */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(250, 249, 245, 255), stop:0.5 rgba(248, 247, 242, 255), stop:1 rgba(245, 244, 240, 255));
    color: rgb(114, 95, 77);
